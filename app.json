{
  "name": "On The Hill",
  "slug": "OnTheHill",
  "scheme": "onthehill",
  "version": "1.0.1",
  "orientation": "portrait",
  "userInterfaceStyle": "automatic",
  "icon": "./assets/images/icon.png",
  "updates": {
    "fallbackToCacheTimeout": 0
  },
  "newArchEnabled": true,
  "jsEngine": "hermes",
  "assetBundlePatterns": [
    "**/*"
  ],
  "android": {
    "icon": "./assets/images/icon.png",
    "package": "com.onthehill",
    "adaptiveIcon": {
      "foregroundImage": "./assets/images/icon.png",
      "backgroundImage": "./assets/images/icon.png"
    },
    "allowBackup": false,
    "edgeToEdgeEnabled": true
  },
  "ios": {
    "buildNumber": "7",
    "icon": "./assets/images/icon.png",
    "supportsTablet": true,
    "bundleIdentifier": "com.onthehill",
    "privacyManifests": {
      "NSPrivacyAccessedAPITypes": [
        {
          "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
          "NSPrivacyAccessedAPITypeReasons": [
            "CA92.1"
          ]
        }
      ]
    },
    "infoPlist": {
      "ITSAppUsesNonExemptEncryption": false
    }
  },
  "web": {
    "favicon": "./assets/images/icon.png",
    "bundler": "metro"
  },
  "plugins": [
    "expo-localization",
    "expo-font",
    [
      "expo-splash-screen",
      {
        "image": "./assets/images/icon-splash.png",
        "imageWidth": 300,
        "resizeMode": "contain",
        "backgroundColor": "#C76542"
      }
    ],
    [
      "react-native-edge-to-edge",
      {
        "android": {
          "parentTheme": "Light",
          "enforceNavigationBarContrast": false
        }
      }
    ],
    "expo-router"
  ],
  "experiments": {
    "tsconfigPaths": true,
    "typedRoutes": true
  },
  "extra": {
    "ignite": {
      "version": "11.0.1"
    },
    "router": {},
    "eas": {
      "projectId": "1cbd11ff-afe4-4c1a-b893-d46b019f82ad"
    }
  }
}
